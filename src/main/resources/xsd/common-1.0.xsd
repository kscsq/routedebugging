			<xs:element name="Coefficient" type="cmn:BaseCoefficientType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Коэффициент</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BrokerRoleListType">
		<xs:annotation>
			<xs:documentation>Тип со списком ролей посредника (привёл клиента, заключил договор и т.п.)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BrokerRole" type="cmn:BrokerRoleType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Роль посредника</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BrokerRoleType">
		<xs:annotation>
			<xs:documentation>Тип для описание одной роли посредника</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="RoleId" type="cmn:ObjIdType">
				<xs:annotation>
					<xs:documentation>Идентификатор роли посредника по отношению к договору страхования

Type= D_BROKERROLE</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BoxContractType">
		<xs:annotation>
			<xs:documentation>Тип:коробочный договор</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BoxId" type="cmn:ObjIdType">
				<xs:annotation>
					<xs:documentation>Идентификатор  коробки
					
Тype = BOX</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsActivated" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Признак активации коробки</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsSold" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Признак продажи коробки</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BoxActivationTypeId" type="cmn:ObjIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Тип активации коробки

SysId=XML
Type=D_BOXACTIVATION</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="StoaType">
		<xs:annotation>
			<xs:documentation>Тип: информация о СТОА</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="StoaId" type="cmn:ObjIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор СТОА

Type = STOA</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Name" type="xs:string">
				<xs:annotation>
					<xs:documentation>Наименование СТОА</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Address" type="cmn:AddressType">
				<xs:annotation>
					<xs:documentation>Адрес СТОА</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Phones" type="cmn:PhonesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Телефона СТОА</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TechnicalDataType">
		<xs:annotation>
			<xs:documentation>Тип для передачи технической составляющей сообщения</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Originator" type="xs:string">
				<xs:annotation>
					<xs:documentation>Код системы-отправителя оригинального сообщения. СКК 6.1.27</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MessageId" type="xs:string">
				<xs:annotation>
					<xs:documentation>Уникальный идентификатор сообщения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CorrelationId" type="xs:string">
				<xs:annotation>
					<xs:documentation>Бизнес идентификатор сообщения. Данный идентификатор позволяет системе инициатору однозначно ассоциировать пришедший ответ с запросом</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ActionId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор атомарной операции</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MessageCode" type="xs:string">
				<xs:annotation>
					<xs:documentation>Код сообщения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MessageType" type="xs:string">
				<xs:annotation>
					<xs:documentation>Тип сообщения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Priority" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Приоритет выгружаемого сообщения. 0 – низший, 9 – высший</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GroupId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Уникальный идентификатор группы сообщений</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GroupSeq" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Уникальный идентификатор сообщения в рамках группы</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Recipients" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Список систем-получателей, разделённых через , или ;</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResponseRequired" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Требуется немедленный ответ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NotificationRequired" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Требуется подтверждение обработки</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PaymentScheduleCoverType">
		<xs:annotation>
			<xs:documentation>Тип для описания оплаты по конкретному покрытию в рамках одной строчки графика платежей</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CoverId" type="cmn:ObjIdType">
				<xs:annotation>
					<xs:documentation>Идентификатор экземпляра страхового покрытия в системе

Type=COVER</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentSum" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>Сумма платежа по покрытию</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="StatementInfoType">
		<xs:annotation>
			<xs:documentation>Информация об отчёта агента</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Number" type="xs:string">
				<xs:annotation>
					<xs:documentation>Номер отчёта агента</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Date" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата отчёта агента</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DiscountType">
		<xs:annotation>
			<xs:documentation>Тип для передачи причины и величины скидки(акция)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DiscountTypeId" type="cmn:ObjIdType">
				<xs:annotation>
					<xs:documentation>Идентификатор  причины скидки.

Ids:
1-Скидка за счёт КВ
2-Абстрактная акция

SysId=XML
Type=DISCOUNTTYPE</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DiscountValueTypeId" type="cmn:ObjIdType">
				<xs:annotation>
					<xs:documentation>Идентификатор типа значения скидки.

Ids:
1-Суммовой (в значении  указывается конкретное значение в валюте договора), например 1500 (рублей)
2-Процентный (указывается процент скидки)
SysId=XML
Type=D_DISCOUNTVALTYPE</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DiscountValue" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>Значение скидки</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Code" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Промокод и т.п.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ObjectStatusType">
		<xs:annotation>
			<xs:documentation>Тип для передачи статуса объекта</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ObjectId" type="cmn:ObjIdType">
				<xs:annotation>
					<xs:documentation>Идентификатор и тип сущности, чей статус передаётся</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StatusId" type="cmn:ObjIdType">
				<xs:annotation>
					<xs:documentation>Идентификатор статуса</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SystemDate" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Дата и время установки статуса в системе</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StatusDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата и время с которых начинает действовать передаваемый статус объекта в системе-источнике (бизнес-дата)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ObjectStatusMessageType">
		<xs:annotation>
			<xs:documentation>Тип для формирования сообщения о статусе объекта</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TechData" type="cmn:TechnicalDataType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Техническая составляющая сообщения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BusinessData">
				<xs:annotation>
					<xs:documentation>Бизнес-данные</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ObjectStatus" type="cmn:ObjectStatusType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemaVersion" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Номер версии схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ChangeSequenceId" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Состояние бизнес-объекта. Должен меняться при каждой операции изменения бизнес значимого состояния объекта</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="ObjectStatus">
		<xs:annotation>
			<xs:documentation>Элемент для передачи статуса объекта</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="cmn:ObjectStatusMessageType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="FileType">
		<xs:annotation>
			<xs:documentation>Тип для передачи прикрепленного документа (файла)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FileId" type="cmn:ObjIdType">
				<xs:annotation>
					<xs:documentation>Идентификатор файла системы, в которой хранится документ

SysId:
Кассандра - FILESTORE
ТЕССА - TESSA
Либо код системы-источника
Type=FILE</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GroupFileId" type="cmn:ObjIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор группы файлов. Используется в том случае, если 1 файл физически разделён на несколько. В этом случае этот идентификатор будет общим для всей группы</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FileNumber" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер файла в группе, используется для того, чтобы в верной последовательности слить файлы в один</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="URL" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Ссылка на документ. Обязательно должен быть заполнен, если отсутствует тег FileBody</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FileName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Имя файла документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FileType" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Тип файла: pdf, doc, docx,jpg,png и т.п.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DocumentName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Имя документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FileBody" type="xs:base64Binary" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Содержимое файла в base64. Обязательно должен присутствовать, если тег URL отсутствует либо пустой</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
