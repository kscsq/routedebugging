<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:pay="http://rgs.ru/schema/payment10" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cmn="http://rgs.ru/schema/common10" targetNamespace="http://rgs.ru/schema/payment10" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.8">
	<xs:import namespace="http://rgs.ru/schema/common10" schemaLocation="common-1.0.xsd"/>
	<xs:complexType name="PaymentPersonType">
		<xs:annotation>
			<xs:documentation>Тип для описания информации о плательщике/получателе</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Person" type="cmn:PersonType">
				<xs:annotation>
					<xs:documentation>Информация о контрагенте</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="InvoiceDataType">
		<xs:annotation>
			<xs:documentation>Тип для описания счёта</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Invoice" type="cmn:InvoiceType">
				<xs:annotation>
					<xs:documentation>Cчет</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Payer" type="cmn:PersonType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Информация о плательщике (кому выставляется счёт)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PayerTypeId" type="cmn:ObjIdType">
				<xs:annotation>
					<xs:documentation>Идентификатор типа плательщика

SysId = XML;
Type = PAYER_TYPE 
Ids: 
1 - Клиент
2 - Посредник</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AgentCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код агента, например, ЛНР</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RecipientName" type="xs:string">
				<xs:annotation>
					<xs:documentation>Наименование получателя</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RecipientAccount" type="pay:InvoiceAccountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Банковские реквизиты получателя</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AgentContractId" type="cmn:ObjIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор договора с агентом

Type=AGENTCONTRACT</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LinkedDocuments" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Ссылки на произволные документы (например, квитанции А7)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="DocumentId" type="cmn:ObjIdType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Contracts" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Информация о договорах страхования, к которым относится данный счёт. Используется для более корректного сопоставления счетов и платёжных документов</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Contract" type="pay:InvoiceContractType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Информация о договоре страхования, к которому относится счёт</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="InvoiceData" type="pay:InvoiceDataType">
		<xs:annotation>
			<xs:documentation>Элемент для передачи информации о счёте</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PaymentOrder" type="pay:PaymentOrderType">
		<xs:annotation>
			<xs:documentation>Элемент для передачи информации о запросе на выплату</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="PaymentOrderType">
		<xs:annotation>
			<xs:documentation>Тип для описания счёта</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PaymentOrderId" type="cmn:ObjIdType">
				<xs:annotation>
					<xs:documentation>Идентификатор распоряжения на выплату

Type=PAYMENTORDER</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrevPaymentOrderId" type="cmn:ObjIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор предыдущего распоряжения на выплату. Заполняется в случае, если текущее распоряжение на выплату является повторным. Нужен для получения ряда атрибутов с предыдущего распоряжения на выплату</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Number" type="xs:string">
				<xs:annotation>
					<xs:documentation>Номер документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BranchCode" type="xs:string">
				<xs:annotation>
					<xs:documentation>Код подразделения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentMethodId" type="cmn:ObjIdType">
				<xs:annotation>
					<xs:documentation>Идентификатор метода оплаты. Справочник в СКК 7.10.22

SysId=SCC2
Type=PAYMENT_OPTION</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Date" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Дата выставления документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Currency" type="cmn:CurrencyType">
				<xs:annotation>
					<xs:documentation>Валюта выплаты</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentSum" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>Сумма платежа в валюте выплаты</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentSumRUB" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сумма платежа в рублях</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentOrderProperties" type="cmn:PropertyListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дополнительные атрибуты счёта(расопряжения на выплату)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Recipient" type="pay:PaymentPersonType">
				<xs:annotation>
					<xs:documentation>Информация о получателе</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PayerName" type="xs:string">
				<xs:annotation>
					<xs:documentation>Наименование плательщика</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Account" type="pay:InvoiceAccountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Банковские реквизиты для осуществления выплаты</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemaVersion" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="PaymentDocumentType">
		<xs:annotation>
			<xs:documentation>Тип для описания платёжного документа</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PaymentDocumentId" type="cmn:ObjIdType">
				<xs:annotation>
					<xs:documentation>Идентификатор экземпляра документа, подтверждающего оплату

Type=PAYMENTDOCUMENT</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentDocumentTypeId" type="cmn:ObjIdType">
				<xs:annotation>
					<xs:documentation>Идентификатор типа платёжного документа. СКК 7.4.33

Type=DICTPAYMENTDOCUMENT</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentMethodId" type="cmn:ObjIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор метода оплаты. Справочник в СКК 7.10.22

SysId=SCC2
Type=PAYMENT_OPTION</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Date" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Дата и время оплаты</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Amount" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>Сумма оплаты</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CurrencyCode">
				<xs:annotation>
					<xs:documentation>Код валюты сопоставления (USD, EUR,RUB и т.п.). Это или валюта счёта или валюта платёжных документов</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Series" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Серия документа, подтверждающего оплату</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Number" type="xs:string">
				<xs:annotation>
					<xs:documentation>Номер документа, подтверждающего оплату</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentPurpose" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Назначение платежа. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AuthCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код авторизации. Используется для сопоставления платежа и счёта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrderNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер заказа. Используется для сопоставления платежа и счёта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RecipientFullName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ФИО получателя денег от клиента</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ActualPayer" type="pay:ActualPayerType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Информация о плательщике из платёжного документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentSumCash" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>Сумма оплаты наличными</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentSumCashless" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>Сумма оплаты безналичным методом</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Description" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Примечания</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Contracts" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Информация о договорах, который оплачитваются/зачитываются и т.п.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Contract" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ContractSeries" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Серия договора</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ContractNumber" type="xs:string">
										<xs:annotation>
											<xs:documentation>Номер договора</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="InsurancePremiumRUB" type="xs:decimal">
										<xs:annotation>
											<xs:documentation>Оплаченная премия по договору</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PaymentNdflDocumentType">
		<xs:annotation>
			<xs:documentation>Тип для описания платёжного документа НДФЛ</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PaymentDocumentId" type="cmn:ObjIdType">
				<xs:annotation>
					<xs:documentation>Идентификатор экземпляра документа, подтверждающего оплату

Type=PAYMENTDOCUMENT</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepartmentId" type="cmn:ObjIdType">
				<xs:annotation>
					<xs:documentation>Идентификатор подразделения по которому начисляются суммы РПВ по сотрудникам 

Type=DEPARTMENT</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PayoutDate" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Дата и время выплаты НДФЛ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Date" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Дата платежного поручения на перечисление НДФЛ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Amount" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>Сумма оплаты</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Series" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Серия документа, подтверждающего оплату</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Number" type="xs:string">
				<xs:annotation>
					<xs:documentation>Номер документа, подтверждающего оплату</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Kpp" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>КПП  подразделения по которому начисляются суммы РПВ по сотрудникам </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Oktmo" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ОКТМО  подразделения по которому начисляются суммы РПВ по сотрудникам </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DocumentContentType">
		<xs:annotation>
			<xs:documentation>Содержимое документа (платёжного, деталзация счёта и т.п.)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DocumentContentId" type="cmn:ObjIdType">
				<xs:annotation>
					<xs:documentation>Идентификатор строки наполнения документа

Type=DOCUMENTCONTENT</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MatchingId" type="cmn:ObjIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор операции сопоставления

Type=MATCHING</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DocumentId" type="cmn:ObjIdType">
				<xs:annotation>
					<xs:documentation>Идентификатор докумета, по которому строится детализация.

Type заполнять в соответствии с типом документа, по которому идёт данная расшифровка, например для счёта это 
INVOICE, а для платёжного документа это
PAYMENTDOCUMENT</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="InsuranceActContent" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Содержимое страхового акта, по которому производится выплата</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="InsuranceActId" type="cmn:ObjIdType">
								<xs:annotation>
									<xs:documentation>Идентификатор страхового акта

Type=INSURANCEACT</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="CoverContent" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Содержимое по покрытию</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CoverId" type="cmn:ObjIdType">
								<xs:annotation>
									<xs:documentation>Идентификатор покрытия</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="CoverTypeId" type="cmn:ObjIdType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Идентификатор типа страхового покрытия

SysID = SCC2
Type 
для АРМ
DICTRISKOBJECTTYPE_22
для ЕКИС
S_RISKS</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ProductId" type="cmn:ObjIdType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Идентификатор Страхового продукта.
В терминах GW это Offering, для остальных систем это продукт

Type = PRODUCT</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MainContractId" type="cmn:ObjIdType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Идентификатор первичного договора в системе-источнике.
Для договора передавать значение из ContractId. Для дополнительных соглашений передавать идентификатор  договора.

Type=CONTRACT</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Amount" type="xs:decimal">
								<xs:annotation>
									<xs:documentation>Сумма оплаты по покрытию</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Currency" type="cmn:CurrencyType">
								<xs:annotation>
									<xs:documentation>Валюта</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="AmountRUB" type="xs:decimal">
								<xs:annotation>
									<xs:documentation>Сумма оплаты по покрытию в рублях</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="PaymentSchedulleId" type="cmn:ObjIdType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Идентификатор строки плана-графика договора</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ClientInvoiceId" type="cmn:ObjIdType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Идентификатор клиентского счёта. Заполняется тогда, когда осуществляется оплата страхователем</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="AgentInvoiceId" type="cmn:ObjIdType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Идентификатор агентского счёта. Заполняется тогда, когда осуществляется оплата агентом</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="PaymentId" type="cmn:ObjIdType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Идентификатор подтверждающего документа, например квитанции А7.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="PaymentDocumentId" type="cmn:ObjIdType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Идентификатор экземпляра документа, подтверждающего оплату

Type=PAYMENTDOCUMENT</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="PaymentDocuments" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Платёжные документы</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="PaymentDocument" type="pay:PaymentDocumentType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Платёжный документ</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemaVersion" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string"/>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="MatchingDocumentType">
		<xs:annotation>
			<xs:documentation>Тип для описания информации о сопоставлении документов</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MatchingId" type="cmn:ObjIdType">
				<xs:annotation>
